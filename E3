import java.util.*;

public class Main {
    
    public static void suma( int n1, int n2, int n3) {
       int resultado = n1 + n2 + n3;
        
        System.out.println(String.valueOf(resultado));
    }
    
    
    
    public static class Coche {
      static int puertas = 4;
      void addPuertas(int nPuertas) {
          puertas += nPuertas;
          System.out.println(String.valueOf(Coche.puertas));
          
      }
    }
      
     
    
    
    
    public static void main(String[] args) {
       suma(1,2,3);
       Coche nuevo = new Coche();
       nuevo.addPuertas(1);
      
    }
}
